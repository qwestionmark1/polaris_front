{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "framework": "vite",
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": { "zeroConfig": true }
      }
    ],
    "routes": [
      {
        "src": "/assets/(.*)",
        "headers": { "cache-control": "public, max-age=31536000, immutable" },
        "continue": true
      },
      {
        "handle": "filesystem"
      },
      {
        "src": "/.*",
        "dest": "/index.html"
      }
    ]
  }